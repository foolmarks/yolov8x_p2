            ],
            "gst": "simaaisrc location=/data/simaai/applications/pipeline3/etc/img%d.nv12 node-name=decoder delay=1000 mem-target=1 index=1 loop=true ! 'video/x-raw, format=(string)NV12, width=(int)1280, height=(int)720' ! tee name=source ! queue2 name=queue2_1 ! simaaiprocesscvu  name=simaaiprocesspreproc_1 ! simaaiprocessmla  name=simaaiprocessmla_1 ! simaaiboxdecode  name=simaaibox_decoder_1 ! 'application/vnd.simaai.tensor' ! simaai-overlay2  name=simaaioverlay_1 render-info='input::decoder,bbox::simaaibox_decoder_1 labels-file='/data/simaai/applications/pipeline3/share/overlay/labels.txt' ! yolov8xp2_postproc_overlay  name='simaai_yolov8xp2_postproc_overlay' ! queue2 name=queue2_3 ! 'video/x-raw, format=(string)RGB, width=(int)1280, height=(int)720' ! fakesink source. ! queue2 name=queue2_2 ! simaaioverlay_1. "
