                    "destination": {
                        "provider": "RPM",
                        "type": "lib"
                    }
                }
            ],
            "gst": "simaaisrc location=/data/simaai/applications/pipeline4/etc/img%d.nv12 node-name=decoder mem-target=1 index=1 delay=500 loop=true ! 'video/x-raw, format=(string)NV12, width=(int)1280, height=(int)720' ! tee name=source ! queue2 name=queue2_1 ! simaaiprocesscvu  name=simaaiprocesspreproc_1 ! simaaiprocessmla  name=simaaiprocessmla_1 ! simaaiboxdecode  name=simaaibox_decoder_1 ! 'application/vnd.simaai.tensor' ! simaai-overlay2  name=simaaioverlay_1 render-info='input::decoder,bbox::simaaibox_decoder_1 labels-file='/data/simaai/applications/pipeline4/share/overlay/labels.txt' ! simaaiencoder enc-fmt=NV12 enc-bitrate=4000 name=encoder ! h264parse ! rtph264pay ! udpsink host=192.168.1.24 port=9000 source. ! queue2 name=queue2_2 ! simaaioverlay_1. "
        }
    ],
    "configuration": {
