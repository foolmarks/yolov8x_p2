{
    "name": "pipeline4",
    "appId": "pipeline4",
    "sequence": 1,
    "pipelines": [
        {
            "name": "pipeline4_Pipeline",
            "sequence": 1,
            "plugins": [
                {
                    "name": "simaaisrc_1",
