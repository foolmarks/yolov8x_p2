        },
        "gst": {
            "options": [
                "--gst-plugin-path=/data/simaai/applications/pipeline4/lib"
            ]
        },
        "environment": [
            "GST_DEBUG=0",
            "LD_LIBRARY_PATH=/data/simaai/applications/pipeline4/lib"
        ]
    },
    "manifestVersion": "2.0.0"
