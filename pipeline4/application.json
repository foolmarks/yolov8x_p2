                    "sequence": 1,
                    "resources": {
                        "configs": [
                            "res/img1.nv12",
                            "res/img2.nv12",
                            "res/img3.nv12",
                            "res/img4.nv12",
                            "res/img5.nv12"
                        ]
                    }
                },
                {
                    "name": "simaaiprocesspreproc_1",
                    "pluginGid": "processcvu",
                    "sequence": 2,
                    "resources": {
                        "configs": [
                            "cfg/0_preproc.json"
                        ]
                    }
                },
                {
                    "name": "simaaiprocessmla_1",
                    "pluginGid": "processmla",
                    "sequence": 3,
                    "resources": {
                        "configs": [
                            "cfg/0_process_mla.json"
